<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="app_bg">
        <item name="android:background">@color/cebebeb</item>
    </style>

    <style name="dialogAnim" parent="android:Animation">
        <item name="@android:windowEnterAnimation">@anim/from_bottom_enter</item>
        <item name="@android:windowExitAnimation">@anim/from_bottom_exit</item>
    </style>
</resources>
